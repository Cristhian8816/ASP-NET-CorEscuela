@*
@model List<Alumno> 
*@

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>  
            <th scope="col">Email</th>
            <th scope="col">Curso</th>           
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Nombre</td>
                <td>@obj.Id</td> 
                <td>@obj.Email</td>   
                <td>@obj.Curso</td>                              
            </tr>
        }
    </tbody>
</table>