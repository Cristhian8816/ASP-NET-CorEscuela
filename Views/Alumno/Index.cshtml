
@model Alumno

@{
    ViewData["Title"] = "Informacion Alumno";
    Layout = "Simple";
}

<h1>@ViewData["Title"]</h1>

<strong>Desde la vista normal</strong>
<p>
    Nombre: @Model.Nombre <br/>
    ID: @Model.UniqueId 
</p>

<strong>Desde la vista Parcial</strong>
<partial name= "ObjetoEscuelaSimple"/>

<p>@ViewBag.CosaDinamica</p>
<p>@ViewBag.Fecha</p>
